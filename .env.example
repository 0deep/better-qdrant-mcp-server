# Qdrant Configuration
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_api_key_if_needed

# MCP Upload Directory
MCP_UPLOAD_DIR=/tmp/mcp-uploads

# OpenAI Embedding Service
OPENAI_API_KEY=your_openai_api_key
OPENAI_ENDPOINT=https://api.openai.com/v1

# OpenRouter Embedding Service
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_ENDPOINT=https://openrouter.ai/api/v1

# Ollama Embedding Service
OLLAMA_ENDPOINT=http://localhost:11434
OLLAMA_MODEL=nomic-embed-text
OLLAMA_API_KEY=your_ollama_api_key_if_needed

# FastEmbed Embedding Service
FASTEMBED_API_KEY=your_fastembed_api_key_if_needed
FASTEMBED_ENDPOINT=http://localhost:8000 # Example for a remote FastEmbed service

# General Embedding Configuration
EMBEDDING_MODEL=default_embedding_model # e.g., text-embedding-ada-002, nomic-embed-text
EMBEDDING_PROVIDER=ollama # e.g., openai, openrouter, ollama, fastembed